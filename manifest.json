{
  "manifest_version": 3,
  "name": "Have a break, Have a Kitty-Kat",
  "description": "Pop Up that allows for a break",
  "version": "1.0",
  "icons": {
    "16": "images/logo_16.png",
    "32": "images/logo_32.png",
    "128": "images/logo_128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/break_extensions.png"
  },
  "permissions": ["nativeMessaging", "storage", "activeTab"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
